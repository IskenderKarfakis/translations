#: .\eulacomponent.cpp
msgid "[Licenser]%(1) End User License Agreement"
msgstr "%(1) Licenční ujednání s konečným uživatelem"

#: .\authorizationhandler.cpp
msgid "[Licenser]%(1) requires access to your $APPCOMPANY user account."
msgstr "%(1) vyžaduje přístup k vašemu uživatelskému účtu $APPCOMPANY."

#: .\usercomponent.cpp
msgid "[Command]Activation"
msgstr "Aktivace"

#: .\usercomponent.cpp
msgid "[Licenser]Additional License Information:\n"
msgstr "Další informace o licenci:\n"

#: .\usercomponent.cpp
msgid "[Command]Application"
msgstr "Aplikace"

#: .\authorizationhandler.cpp
msgid "[Licenser]Authorization"
msgstr "Autorizace"

#: .\usercomponent.cpp
msgid "[Licenser]Copy to Clipboard"
msgstr "Kopírovat do schránky"

#: .\usercomponent.cpp
msgid "[Licenser]Invalid License File!"
msgstr "Neplatný soubor licence!"

#: .\usercomponent.cpp
msgid "[Licenser]License too old. Please activate $APPNAME again."
msgstr "Licence je stará. Aktivujte prosím $APPNAME znovu."

#: .\activationhandler.cpp
msgid "[Licenser]Please fill out the form completely."
msgstr "Vyplňte prosím formulář kompletně."

#: .\usercomponent.cpp
msgid "[Licenser]Please restart $APPNAME for the changes to take effect."
msgstr "Aby se změny projevily, restartujte prosím $APPNAME."

#: .\activationhandler.cpp
msgid "[Licenser]Refresh"
msgstr "Obnovit"

#: .\usercomponent.cpp
msgid ""
"[Licenser]Thank You! $APPNAME has been activated to run on this computer."
msgstr ""
"Děkujeme! Aplikace $APPNAME byla aktivována pro použití na tomto počítači."

#: .\activationhandler.cpp
msgid ""
"[Licenser]Thank You! An E-Mail has been sent to you with a link to activate "
"your account."
msgstr "Děkujeme! Byl vám zaslán e-mail s odkazem pro aktivaci vašeho účtu."

#: .\usercomponent.cpp
msgid "[Licenser]The License has expired!"
msgstr "Licence vypršela!"

#: .\usercomponent.cpp
msgid "[Licenser]The License has not been issued for this computer!"
msgstr "Tato licence nebyla vydána pro tento počítač!"

#: .\usercomponent.cpp
msgid "[Licenser]This copy is licensed to %(1)"
msgstr "Tato kopie je licencovaná pro %(1)"

#: .\userlicense.cpp
msgid "[FileType]User License File"
msgstr "Soubor uživatelské licence"

#: .\usercomponent.cpp
msgid "[Licenser]User License not found! Please select a new License File."
msgstr ""
"Licence uživatele nebyla nalezena! Vyberte prosím nový licenční soubor."

#: .\usercomponent.cpp
msgid "[Licenser]Verifying User License..."
msgstr "Ověřování licence uživatele..."
